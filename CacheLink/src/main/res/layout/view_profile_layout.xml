<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Profile ID"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_id"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Gender"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_gender"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Name"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Email"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_email"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Username"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_username"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Register time"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_reg_time"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Date of birth"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_dob"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="School"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_school"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Mobile"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_mobile"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp100"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Profile Image"
                android:gravity="center|right"/>
            <ImageView
                android:id="@+id/profile_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@android:drawable/ic_secure"
                android:gravity="center"
                android:scaleType="centerInside"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="User type"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_role"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Rating"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_rate"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/dp100"
                android:layout_height="match_parent"
                android:text="Rating Total"
                android:gravity="center|right"/>
            <TextView
                android:id="@+id/profile_rate_total"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>